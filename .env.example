ENV=[DEV/PROD/UAT]
ALLOWED_HOSTS=localhost.example.com;example.example.com
CORS_ALLOWED_ORIGINS=http://example.in:3000;http://example.com:3000
CSRF_TRUSTED_ORIGINS=http://example.in:3000;http://example.com:3000

# Sentry initialization will only trigger if SENTRY_DSN variable is present
SENTRY_DSN=https://{VALUE}.ingest.us.sentry.io/{VALUE}

KNOCK_API_KEY=knock-production-api-key

# DB Configuration
ENGINE=django.db.backends.{DATABASE_ENGINE}
NAME={DB_NAME}
USER={DB_USERNAME}
PASSWORD={DB_PASSWORD}
HOST={DB_HOST}
PORT={DB_PORT}
SSL_MODE_REQUIRED={True|False}
DISABLE_SERVER_SIDE_CURSORS={True|False}