    "role": "Assosiate Insultant",
    "github_url": "https://letraz.app",
    "live_url": "https://subhajit.lol",
    "started_from_month": 1,
    "started_from_year": 2021,
    "finished_at_month": 11,
    "finished_at_year": 2022,
    "current": false
}

